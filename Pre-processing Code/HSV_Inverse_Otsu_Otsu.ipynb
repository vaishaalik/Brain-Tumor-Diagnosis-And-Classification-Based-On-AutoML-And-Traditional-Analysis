        "\n",
        "    class_labels = ['license plate']\n",
        "\n",
        "    for filename in os.listdir(path):\n",
        "        if filename.endswith(\".jpg\"):\n",
        "            # loading image\n",
        "            image = cv2.imread(os.path.join(path, filename))\n",
        "            # try:\n",
        "            height, width = image.shape[:2]\n",
        "            # except:\n",
        "            #   print ('Caught issue')\n",
        "            #   continue\n",
        "            # else:\n",
        "            #   print('Issa oki')\n",
        "            # loading label text file\n",
        "            \"\"\"bbox = []\n",
        "            with open(boxpath + \"\\\\\" + filename[:-3] + \"txt\") as f:\n",
        "                lines = f.readlines()\n",
        "            \n",
        "            for line in lines:\n",
        "                bbox.append(list(map(float, line.rstrip().split(' '))))\"\"\"\n",
        "\n",
        "            # defining transform\n",
        "            transform = A.Compose([\n",
        "                #A.MotionBlur(always_apply=True, p=1.0, blur_limit=(25, 25))\n",
        "\n",
        "                #A.RandomGamma(gamma_limit=(295, 300), eps=None, always_apply=True, p=1),\n",
        "\n",
        "                #A.RandomRotate90(always_apply=False, p=1.0),\n",
        "\n",
        "                #A.ElasticTransform(always_apply=True, p=1.0, alpha=1.0, sigma=25.0, alpha_affine=27.0, interpolation=2, border_mode=5, approximate=False),\n",
        "\n",
        "                #A.ShiftScaleRotate(always_apply=False, p=1.0, shift_limit=(-0.06, 0.06), scale_limit=(-0.09999999999999998, 0.10000000000000009), rotate_limit=(-90, 90), interpolation=0, border_mode=0, value=(0, 0, 0), mask_value=None)\n",
        "\n",
        "                #A.RandomBrightnessContrast (brightness_limit=1.5, contrast_limit=0.02, always_apply=False, p=1.0),\n",
        "\n",
        "                #A.RandomCrop (height, width, always_apply=True, p=1.0)\n",
        "\n",
        "                #A.CLAHE (clip_limit=4.0, tile_grid_size=(8, 8), always_apply=False, p=1.0) #Y Channel Histogram Equivalent\n",
        "\n",
        "                A.HueSaturationValue(hue_shift_limit=60, sat_shift_limit=70, val_shift_limit=27, p=1.0) #HSV\n",
        "\n",
        "                #A.ToGray(p=1.0)\n",
